<?xml version="1.0"?>
<package format="2">
  <name>move_base_z_client_plugin</name>
  
  <version>0.9.1</version>
  <description>The move_base_z_client_plugin package implements SMACC Action Client Plugin for the ROS Navigation State - move_base node.  Developed by Reel Robotics.</description>

  <author email="pablo@ibrobotics.com">Pablo Inigo Blasco</author>
  <maintainer email="pablo@ibrobotics.com">Pablo Inigo Blasco</maintainer>

  <license>MIT</license>

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>pluginlib</build_depend>
  <exec_depend>pluginlib</exec_depend>
  <build_depend>smacc</build_depend>
  <exec_depend>smacc</exec_depend>
  <depend>tf</depend>
  <!-- <depend>yaml-cpp</depend> -->

  <export>
      <smacc plugin="${prefix}/smacc_plugin.xml" />
      <rosdoc config="rosdoc.yaml" />
  </export>
</package>
